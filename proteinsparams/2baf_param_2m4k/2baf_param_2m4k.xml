<set_up>
	<inputs>
		<protein_name>2baf_param_2m4k</protein_name>
		<seq_file>2baf.seq</seq_file>
		<upl_file>2baf_concat_dist.upl</upl_file>
		<hbond_file>2baf_concat_hBond.upl</hbond_file>
		<ang_file>2baf_concat_dihed.aco</ang_file>
 		<upl_xplor>2baf_concatupl_noe.tbl</upl_xplor>
 		<aco_xplor>2baf_concat_dihed_aco.tbl</aco_xplor>
		<protein_path>../protein/2baf_param_2m4k</protein_path>
	</inputs>
	<params>
		<hydrogen_omission>1</hydrogen_omission>
		<aug_bounds>0</aug_bounds>
		<break_graph>
			<eta_lo>0.7</eta_lo>
			<eta_hi>1</eta_hi>
		</break_graph>
		<include_neighbour>8</include_neighbour>
		<multi_expand>
			<k_2>3</k_2>
			<size_cutoff>0.1</size_cutoff>
			<grp_min>250</grp_min>
			<incr_min>2</incr_min>
		</multi_expand>
		<grp_expand>30</grp_expand>
	</params>
</set_up>
