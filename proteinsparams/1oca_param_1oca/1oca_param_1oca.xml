<set_up>
	<inputs>
		<protein_name>1oca_param_1oca</protein_name>
		<seq_file>1oca.seq</seq_file>
		<upl_file>1oca_concatupl.upl</upl_file>
		<hbond_file></hbond_file>
		<ang_file></ang_file>
 		<upl_xplor>1oca_concate_noe.tbl</upl_xplor>
 		<aco_xplor></aco_xplor>
		<protein_path>../protein/1oca_param_1oca</protein_path>
	</inputs>
	<params>
		<hydrogen_omission>0</hydrogen_omission>
		<aug_bounds>6.5</aug_bounds>
		<break_graph>
			<eta_lo>0.7</eta_lo>
			<eta_hi>1</eta_hi>
		</break_graph>
		<include_neighbour>10</include_neighbour>
		<multi_expand>
			<k_2>3</k_2>
			<size_cutoff>0.1</size_cutoff>
			<grp_min>250</grp_min>
			<incr_min>2</incr_min>
		</multi_expand>
		<grp_expand>50</grp_expand>
	</params>
</set_up>
