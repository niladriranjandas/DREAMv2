<set_up>
	<inputs>
		<protein_name>1f2h_param_1d8v</protein_name>
		<seq_file>1f2h.seq</seq_file>
		<upl_file>1f2h_concat_dist.upl</upl_file>
		<hbond_file>1f2h_concat_hBond.upl</hbond_file>
		<ang_file></ang_file>
 		<upl_xplor>1f2h_concatupl_noe.tbl</upl_xplor>
 		<aco_xplor></aco_xplor>
		<protein_path>../protein/1f2h_param_1d8v</protein_path>
	</inputs>
	<params>
		<hydrogen_omission>1</hydrogen_omission>
		<aug_bounds>7.5</aug_bounds>
		<break_graph>
			<eta_lo>0.6</eta_lo>
			<eta_hi>1</eta_hi>
		</break_graph>
		<include_neighbour>6</include_neighbour>
		<multi_expand>
			<k_2>3</k_2>
			<size_cutoff>0.1</size_cutoff>
			<grp_min>250</grp_min>
			<incr_min>2</incr_min>
		</multi_expand>
		<grp_expand>45</grp_expand>
	</params>
</set_up>
