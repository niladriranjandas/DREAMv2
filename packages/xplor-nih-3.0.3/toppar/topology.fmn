 IMPRoper C4   C4A  C10  N1        IMPRoper N3   C4   C4A  C10
 IMPRoper C2   N3   C4   C4A       IMPRoper N1   C2   N3   C4
 IMPRoper C10  N1   C2   N3        IMPRoper C4A  C10  N1   C2
 IMPRoper O2   N1   N3   C2        IMPRoper H3   C2   C4   N3
 IMPRoper O4   C4A  N3   C4
 IMPRoper O4'  C5'  C3'  C4'       IMPRoper O3'  C2'  C4'  C3'
 IMPRoper O2'  C3'  C1'  C2'
 IMPRoper N10  C10  C4A  N5        IMPRoper N5   C5A  C9A  N10
 IMPRoper C9A  C6   N5   C5A       IMPRoper C9   N10  C5A  C9A
 IMPRoper C6   C7   C8   C9        IMPRoper C7   C8   C9   C9A
 IMPRoper C8   C9   C9A  C5A       IMPRoper C9   C9A  C5A  C6
 IMPRoper C9A  C5A  C6   C7        IMPRoper C5A  C6   C7   C8
 IMPRoper C7M  C8   C6   C7        IMPRoper C8M  C7   C9   C8
 DONOr H3   N3          DONOr H2'  O2'          DONOr H3'  O3'
 DONOr H4'  O4'
 ACCEptor OF1  PF       ACCEptor OF2  PF        ACCEptor OF3  PF
 ACCEptor O5'  PF       ACCEptor O2'  C2'       ACCEptor O3'  C3'
 ACCEptor O4'  C4'      ACCEptor N1             ACCEptor N5
 ACCEptor O2   C2       ACCEptor O4   C4
 END {FMN}
!-----------------------------------------------------------------------
set echo=true end
