GROUP
ATOM N1 TYPE=N CHARGE = 0.0 END
ATOM N2 TYPE=N CHARGE = 0.0 END
ATOM N3 TYPE=N CHARGE = 0.0 END
     
BOND N1 N2   BOND N2 N3
     
ANGLE N1 N2 N3
     
END { AZIDE }
     
