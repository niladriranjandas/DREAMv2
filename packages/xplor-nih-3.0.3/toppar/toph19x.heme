  ANGLe 1O1  2FE  2NA
  ANGLe 1O1  2FE  2NB
  ANGLe 1O1  2FE  2NC
  ANGLe 1O1  2FE 2ND
  DIHEdral 1O2  1O1  2FE  2NA
END {* PLO2 *}
     
