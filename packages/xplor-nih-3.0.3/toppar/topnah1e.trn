REMARKS toptrnah  topology append file for TRNA
REMARKS use with TOPNAH1*, PARNAH1*

set echo=false end
autogenerate angles=false dihedrals=false end
     
!                          NOTE: NO DONORS OR ACCEPTORS FOR TIPS
! PATCHES TO BE APPENDED TO TOPRNA5 MAKING IT TOPTRNA5
! All the modified nucleotides in tRNA are implemented
! as patches ('TYPE II').
! ------------ work in progress ----------------------
!                   /LN
!
! No additional atomtypes seem to be necessary
!
!!!!  ****>>>>CHARGES NEED CHECKING (and atomtypes too)
!             SUBDIVISIONS ARE STILL MISSING
!
!-----------------------------------------------------------
!
     
PRESidue DHU             ! Make Dihydro-U out of U
!        ===

GROUP
   MODIFY ATOM C5   TYPE=C2   CHARge=-0.07  END
   ADD ATOM H51  TYPE=H    CHARge=0.035  END
   ADD ATOM H52  TYPE=H    CHARge=0.035  END
   DELETE ATOM H5 END                        !! Lennart left this out 
GROUP
   MODIFY ATOM C6   TYPE=C2   CHARge=-0.07  END
   ADD ATOM H61  TYPE=H    CHARge=0.035  END
   ADD ATOM H62  TYPE=H    CHARge=0.035  END
   DELETE ATOM H6 END                        !! Lennart left this out 
     
ADD BOND C5 H51   
ADD BOND C5 H52
ADD BOND C6 H61
ADD BOND C6 H62
ADD ANGLE C4 C5 H51    
ADD ANGLE C4 C5 H52
ADD ANGLE C6 C5 H51    
ADD ANGLE C6 C5 H52    
ADD ANGLE H51 C5 H52
ADD ANGLE C5 C6 H61    
ADD ANGLE C5 C6 H62
ADD ANGLE N1 C6 H61    
ADD ANGLE N1 C6 H62     
ADD ANGLE H61 C6 H62
DELE IMPROPER N1   C2   N3   C4  ! The ring is no longer aromatic
DELE IMPROPER C2   N3   C4   C5
DELE IMPROPER N3   C4   C5   C6
DELE IMPROPER C4   C5   C6   N1
DELE IMPROPER C5   C6   N1   C2
DELE IMPROPER C6   N1   C2   N3
ADD DIHE  N1   C2   N3   C4       ! So we use plain dihedrals instead
ADD DIHE  C2   N3   C4   C5
ADD DIHE  N3   C4   C5   C6
ADD DIHE  C4   C5   C6   N1
ADD DIHE  C5   C6   N1   C2
ADD DIHE  C6   N1   C2   N3
! NO DIHEDRALS FOR OUR NEW HYDROGEN FRIENDS  (YET)
END {DHU}

